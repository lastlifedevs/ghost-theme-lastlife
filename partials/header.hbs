<header class="site-header outer" {{#is "index"}}data-0="background: rgba(21, 24, 40, 0);" data-100="background: rgba(21, 24, 40, 0);" data-200=" 
    background: rgba(21, 24, 40, 1);"{{/is}}>
    <span class="site-header-border" {{#is "index"}}data-0="opacity:0;" data-100="opacity:0;" data-200="opacity:1;"{{/is}}></span>
    <div class="inner">
        <div class="site-header-content">
            <div class="scaleTitle" {{#is "index"}}data-0="left:50%; -webkit-transform:scale(1) translateX(-50%); transform:scale(1) translateX(-50%);" data-200="left: 0%; -webkit-transform:scale(0.5) translateX(0%); transform:scale(0.5) translateX(0%);"{{/is}}>
                <a href="/">
                    <div class="mainTitle" id="logo">
                        <div class="bubbleback" id="oneback">
                            <div class="bubble" id="one">
                                <div class="hpmask">
                                    <div class="hp"></div>
                                </div>
                            </div>
                        </div>
                        <div class="bubbleback" id="twoback">
                            <div class="bubble" id="two"></div>
                        </div>
                        <div class="bubbleback" id="threeback">
                            <div class="bubble" id="three">
                                <div class="bubbleText">
                                    Last
                                </div>
                            </div>
                        </div>
                        <div class="bubbleback" id="fourback">
                            <div class="bubble" id="four">
                                <div class="bubbleText">
                                    Life
                                </div>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
            <input class="burger-check" id="burger-check" type="checkbox">
            <label for="burger-check" class="burger"></label>
            <nav class="site-nav" {{#is "index"}}data-0="opacity:1; top:170px; left:50%; -webkit-transform:scale(1) translateX(-50%); transform:scale(1) translateX(-50%);" data-5="opacity:1;" data-15="opacity:0;" data-50="opacity:0; -webkit-transform:scale(1) translateX(-50%); transform:scale(1) translateX(-50%);" data-100="top:9px; left:6%; opacity:0; -webkit-transform:scale(0.6) translateX(10%); transform:scale(0.6) translateX(10%);" data-210="opacity:1;"{{/is}}>
                <ul id="navbar" {{#is "index"}}data-0="left:0%;" data-25="left:0%;" data-50="left:13%;"{{/is}}>
                    <li>
                        <a href="/">Home</a>
                    </li>
                    <li>
                        <a href="/">Archive</a>
                    </li>
                    <li>
                        <a href="/about">About</a>
                    </li>
                    <li>
                        <a href="/forum/">Forum</a>
                    </li>
                </ul>
            </nav>
        </div>
    </div>
</header>